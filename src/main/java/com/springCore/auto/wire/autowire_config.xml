<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean class="com.springCore.auto.wire.Address" name="address">
		<property name="street" value="punamalle road"/>
		<property name="city" value="chennai" /> 
	</bean>
	
	<bean class="com.springCore.auto.wire.Employee" name="emp" autowire="constructor">
		<property name="name" value="deepak" />
	</bean>
	
</beans>